{
	"name": "svelte_template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"pre:dev": "pnpm i",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"pwa:generate": "pwa-assets-generator --preset minimal-2023 static/logo-removebg-preview.png",
		"icones:generate": "cd node_modules/tailwind-icones-plugin && pnpm generate:icones \"%cd%\"",
		"linux:icones:generate": "current_dir=$(pwd) && cd node_modules/tailwind-icones-plugin && pnpm generate:icones \"$current_dir\""
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/adapter-vercel": "^5.3.0",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@total-typescript/ts-reset": "^0.5.1",
		"@typescript-eslint/parser": "^7.8.0",
		"@vite-pwa/assets-generator": "^0.2.4",
		"autoprefixer": "^10.4.16",
		"daisyui": "^4.10.5",
		"dotenv": "^16.4.5",
		"eslint": "^9.2.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.38.0",
		"postcss": "^8.4.31",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.0.5",
		"svelte-check": "^3.7.1",
		"tailwind-icones-plugin": "^1.1.4",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	},
	"type": "module"
}
